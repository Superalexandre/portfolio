{
    "name": "remix-fastify",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "remix build",
        "dev-js": "remix dev --manual -c \"node --watch-path ./server.js --enable-source-maps ./server.js\"",
        "dev": "remix dev --manual -c \"node --no-warnings --loader ts-node/esm --watch-path ./app.ts --enable-source-maps ./app.ts\"",
        "start": "node --enable-source-maps ./server.js",
        "typecheck": "tsc",
        "lint": "eslint --ext .js,.jsx,.ts,.tsx ."
    },
